
<form action="/activities" method="POST">
  <div class="form-group">
    <label for="authEmail">Organization</label>
    <input id="authEmail" class="form-control" type="text" name="organization">
  </div>

  <div class="form-group">
    <label for="">Name</label>
    <input id="authName" class="form-control" type="text" name="name">
  </div>

  <div class="form-group">
    <label for="authPassword">Location</label>
    <input id="authPassword" class="form-control" type="text" name="location">
  </div>

  <div class="form-group">
    <label for="authEmail">Date</label>
    <input id="authEmail" class="form-control" type="date" name="date">
  </div>

  <div class="form-group">
    <label for="authName">Hours</label>
    <input id="authName" class="form-control" type="text" name="hours">
  </div>

  <div class="form-group">
    <label for="authPassword">Event</label>
    <input id="authPassword" class="form-control" type="text" name="event">
  </div>

   <input class="btn btn-primary" type="submit">
</form>
